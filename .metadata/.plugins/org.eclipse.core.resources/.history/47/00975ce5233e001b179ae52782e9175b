
public class ExamReview {
		
	public static void/*int[][]*/ multiplicationTable(int n) {
		
		for(int row = 1; row <= n; row++) {
			
			for(int col = 1; col <= n; col++) {
				
				System.out.println((row * col) + " ");
				
			}
			
			System.out.println();
			
		}
		
		
	}

	
	
	public static void main(String[] args) {

		multiplicationTable(5);
		
	}
	
}
